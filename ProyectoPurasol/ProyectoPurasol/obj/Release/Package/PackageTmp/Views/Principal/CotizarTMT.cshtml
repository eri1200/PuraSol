                                                            if (resultado.success) {

                                                                $("#Cliente").val(resultado.resul[0].Nombre);
                                                                $("#ID").val(document.getElementById("IdCliente").value);
                                                                alert("SE AGREGÓ CORRECTAMENTE");
                                                            } else {
                                                                $("#Cliente").val("");
                                                                alert("ERROR BUSCANDO EL CLIENTE");
                                                            }

                                                        },
                                                        error: function (ex) {
                                                            $("#Cliente").val("");
